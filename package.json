{
  "name": "graphql-test",
  "private": true,
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^5.0.0",
    "babel-plugin-transform-class-properties": "^6.1.20",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": ">=12.0.0",
    "eslint": "^2.4.0",
    "eslint-plugin-react": "^4.2.1",
    "exorcist": ">=0.4.0",
    "local-web-server": "^1.2.1",
    "uglify": "^0.1.5",
    "watchify": ">=3.6.0"
  },
  "engines": {
    "node": "0.10.x"
  },
  "babel": {
    "plugins": [
      "./plugins/babelRelayPlugin",
      "transform-class-properties",
      "transform-decorators-legacy"
    ],
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ]
  },
  "dependencies": {
    "babel-core": "^6.7.2",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-relay-plugin": "^0.7.3",
    "babel-runtime": "^6.6.1",
    "bootstrap": "^3.3.6",
    "graphql": "^0.4.18",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^2.2.0",
    "react": "^0.14.7",
    "react-addons-css-transition-group": "^0.14.7",
    "react-bootstrap": "^0.28.3",
    "react-dom": "^0.14.7",
    "react-redux": "^4.1.2",
    "react-relay": "^0.7.3",
    "redux": "^3.1.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1"
  },
  "scripts": {
    "start": "ws",
    "update-schema": "babel-node ./scripts/updateSchema.js",
    "watch": "watchify js/index.js -t [ babelify ] -o build/app.js --debug --verbose",
    "build": "browserify js/index.js -t [ babelify ] --debug | exorcist build/app.js.map | uglifyjs -c > build/app.js",
    "dev": "npm run update-schema && npm run watch"
  }
}
